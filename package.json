{
  "name": "atlantis-client",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "grammar": "npm run grammar-basic && npm run grammar-unit && npm run grammar-unit-event",
    "grammar-basic": "nearleyc static/javascript/parser/grammar-base/index.ne -o static/javascript/parser/grammar-base-compiled.js",
    "grammar-unit": "nearleyc static/javascript/parser/grammar-unit/index.ne -o static/javascript/parser/grammar-unit-compiled.js",
    "grammar-unit-event": "nearleyc static/javascript/parser/grammar-unit-event/index.ne -o static/javascript/parser/grammar-unit-event-compiled.js",
    "test": "npm run grammar && npm run lint && npm run check-types",
    "build": "npm run grammar && NODE_ENV=production webpack --production",
    "check-types": "tsc",
    "lint": "tslint static/**/*.{ts,tsx}",
    "start": "NODE_ENV=development webpack-dev-server --colors --progress --inline --hot"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,scss,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artyomtrityak/atlantis-client.git"
  },
  "author": "Artem Trytiak <art.trityak@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "ag-grid-community": "^20.2.0",
    "ag-grid-react": "^20.2.0",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "deep-diff": "^1.0.2",
    "gsap": "^2.0.2",
    "js-file-download": "^0.4.5",
    "lodash": "^4.17.11",
    "nearley": "^2.15.1",
    "noty": "^3.2.0-beta",
    "popper.js": "^1.14.4",
    "react": "^16.8.0",
    "react-data-grid": "^5.0.4-2",
    "react-dom": "^16.8.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "tooltip.js": "^1.3.0",
    "utility-types": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/classnames": "^2.2.6",
    "@types/deep-diff": "^1.0.0",
    "@types/gsap": "^1.20.0",
    "@types/lodash": "^4.14.118",
    "@types/nearley": "^2.11.0",
    "@types/react": "^16.8.2",
    "@types/react-data-grid": "^4.0.0",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^6.0.9",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/parser": "^1.2.0",
    "acorn": "^6.0.4",
    "babel": "^6.23.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.13.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^1.0.1",
    "file-loader": "^2.0.0",
    "husky": "^1.1.3",
    "image-webpack-loader": "^4.5.0",
    "lint-staged": "^8.0.4",
    "node-sass": "^4.10.0",
    "prettier": "^1.14.3",
    "sass-loader": "^7.1.0",
