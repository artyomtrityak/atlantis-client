{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "module": "commonjs",
      "target": "esnext",
      "jsx": "react",
      "moduleResolution": "node",
      "allowJs": true,
      "noEmit": true,
      "strict": true,
      "isolatedModules": true,
      "esModuleInterop": true
  },
  "include": [
      "./static/**/*",
      "./typed/**/*"
  ]
}